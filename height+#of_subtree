vector<int>adj[N];
int dep[N],vis[N],sz[N];
void dfs(int u, int p) {
    sz[u] = 1;
    dep[u] = 1 + dep[p];
    for (int v: adj[u]) {
        if (v==p)   continue;
        dfs(v, u);
        sz[u] += sz[v];
    }
    //score.push_back(dep[u]-sz[u]);
}

 int n , m ;
    cin>>n>>m;
    for(int i = 0 ; i < m; i++){
        int a , b ;
        cin>>a>>b;
        adj[a].push_back(b);
        adj[b].push_back(a);
    }
